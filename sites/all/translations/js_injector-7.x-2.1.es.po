msgid "Title"
msgstr "Título"
msgid "Pages"
msgstr "Páginas"
msgid "Delete"
msgstr "Eliminar"
msgid "Actions"
msgstr "Acciones"
msgid "Cancel"
msgstr "Cancelar"
msgid "Description"
msgstr "Descripción"
msgid "This action cannot be undone."
msgstr "Esta acción no se puede deshacer."
msgid "Save"
msgstr "Guardar"
msgid "Are you sure you want to delete %title?"
msgstr "¿Está seguro de que quiere eliminar %title?"
msgid "All"
msgstr "Todo"
msgid "Header"
msgstr "Encabezado"
msgid "Footer"
msgstr "Pie de página"
msgid "Location"
msgstr "Ubicación"
msgid "Media"
msgstr "Multimedia"
